<h2>User profile</h2>

<%= form_for @user, url: step4_signup_index_path, method: :get do |f| %>

  <div class="field">
    <%= f.label :お名前 %> <br />
    <%= f.text_field :family_name, autofocus: true %>
    <%= f.text_field :first_name, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :お名前カナ %> <br />
    <%= f.text_field :family_name_kana, autofocus: true %>
    <%= f.text_field :first_name_kana, autofocus: true %>
  </div>


  <div class="field">
    <%= f.label :郵便番号 %><br />
    <%= f.text_field :postal_code %>
  </div>


  <div class="field">
    <%= f.label :都道府県 %><br />
    <%= f.text_field :prefecture %>
  </div>

  <div class="field">
    <%= f.label :市区町村 %><br />
    <%= f.text_field :city %>
  </div>

  <div class="field">
    <%= f.label :番地 %><br />
    <%= f.text_field :block %>
  </div>


  <div class="field">
    <%= f.label :建物 %><br />
    <%= f.text_field :building %>
  </div>


  <div class="actions">
    <%= f.submit "次へ" %>
  </div>
<% end %>