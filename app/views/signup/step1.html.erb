<h2>User profile</h2>

<%= form_for @user, url: step2_signup_index_path, method: :get do |f| %>
  
  <div class="field">
    <%= f.label :nickname %> <br />
    <%= f.text_field :nickname, autofocus: true, autocomplete: "nickname" %>
  </div>







  <div class="actions">
    <%= f.submit "次へ" %>
  </div>
<% end %>


